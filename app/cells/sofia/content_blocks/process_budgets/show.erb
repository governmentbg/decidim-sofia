<div id="<%= block_id %>" class="home__section">

    <style>
        .district-column {
            grid-template-columns: none;
        }
        .district-section {
            padding-bottom: 0 !important;
        }
        #html-block-process-budgets > div > div > div > div > h3 > a > svg {
            display: inline !important;
        }
        #html-block-process-budgets.home__section {
            background-color: rgb(var(--primary-rgb) / 1);
            padding: 0rem 0rem 1rem 0rem !important;
            max-width: 100vw !important;
            color: #fff;
        }

        h3.home__section-participate-grid__title > .p-4:hover,h3.home__section-participate-grid__title > .p-4.active {
            text-decoration: underline;
        }
    </style>

    <div class="home__section district-section">
      <h2 class="h2 decorator hidden " style="display: block;"><font style="vertical-align: inherit;">
        <%= t(:name, scope: i18n_scope) %>
      </font></h2>


      <div class="" style="margin-top: 30px; text-align: center; border-bottom: 1px solid #cacaca; ">
        <h3 class="home__section-participate-grid__title">
          <% budget = budgets.first %>
          <%= link_to resource_locator(budget).path, class: "p-4", data:{scope_code: '00'} do %>
            <font style="vertical-align: inherit;">
              <%= decidim_escape_translated(budget.title) %>
            </font>
            <svg style="display: inline; right: inherit; top: inherit; bottom: inherit;" width="1em" height="1em" role="img" aria-hidden="true" class="fill-current">
              <use href="/decidim-packs/media/images/remixicon.symbol-e643e553623ffcd49f94.svg#ri-arrow-right-line"></use>
            </svg>
          <%end %>
        </h3>
      </div>

