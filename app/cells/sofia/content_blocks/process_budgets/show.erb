<script>

    document.addEventListener("DOMContentLoaded", function(event) {
      // Tooltip functionality using JavaScript


      const links = document.querySelectorAll('.areas-list a[data-scope-code],a[data-scope-code="00"]');
      links.forEach(path => {
          path.addEventListener('mouseenter', (event) => {

              if (event.target.getAttribute('data-scope-code') == '00') {
                  document.querySelectorAll('.svg-map-wrapper a[data-scope-code]').forEach(function(item) {
                      item.classList.add('active');
                  });
              } else {
                  var href = document.querySelector('.svg-map-wrapper a[data-scope-code="'+event.target.getAttribute('data-scope-code')+'"]');
                  href.classList.add('active');
              }
          });

          path.addEventListener('mouseleave', (event) => {
              document.querySelectorAll('.svg-map-wrapper a.active').forEach(function(item) {
                  item.classList.remove('active');
              });
          });

      });

      const paths = document.querySelectorAll('.path-link');
      const tooltip = document.getElementById('tooltip');
        // debugger;
      paths.forEach(path => {
          path.addEventListener('mouseenter', (event) => {
              //
              // document.querySelectorAll('.areas-list a[data-scope-code]').forEach(function(item) {
              //   item.classList.remove('active');
              // });


              var href = document.querySelector('.areas-list a[data-scope-code="'+event.target.getAttribute('data-scope-code')+'"]');
              href.classList.add('active');

              event.target.setAttribute('href', href.getAttribute('href'));

              const tooltipText = event.target.getAttribute('data-name');
              tooltip.textContent = tooltipText;

              // Position the tooltip near the path
              const pathRect = event.target.getBoundingClientRect();
              // debugger;
              // tooltip.style.left = `${pathRect.left + window.scrollX + pathRect.width / 2}px`;
              // tooltip.style.top = `${pathRect.top + window.scrollY - 30}px`;

              var x = event.clientX - pathRect.left; //x position within the element.
              var y = event.clientY - pathRect.top;  //y position within the element.

              tooltip.style.left = `${x}px`;
              tooltip.style.top = `${y}px`;

              // tooltip.style.display = 'block'; // Make tooltip visible
          });

          path.addEventListener('mouseleave', () => {
              tooltip.style.display = 'none'; // Hide tooltip when mouse leaves
              document.querySelectorAll('.areas-list a.active').forEach(function(item) {
                  item.classList.remove('active');
              });

          });
      });
    });

